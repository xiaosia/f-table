{
  "name": "f-table-plus",
  "author": "xiaosi",
  "homepage": "https://github.com/xiaosia/f-table",
  "description": "基于element plus封装的表格",
  "private": false,
  "version": "0.0.2",
  "main": "./dist/f-table-plus.umd.ts",
  "module": "./dist/f-table-plus.es.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/f-table-plus.es.ts",
      "require": "./dist/f-table-plus.umd.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite ",
    "build": "vue-tsc --noEmit && vite build",
    "product": "vue-tsc --noEmit && vite build ./build/production.env.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.0.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@vitejs/plugin-vue": "^2.2.0",
    "sass": "^1.49.7",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.0",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.29.8"
  }
}
